<div id="repo-container" class="container">
  <section id="repo-header">
    <hgroup>
      <h1><%=repoInfo.nameo%></h1>
      <h4><%= repoInfo.description %> </h4>
      <a class="btn" href="<%= repoInfo.html_url %>">View Repo</a>
    </hgroup>
  </section>
  <section id="repo-stats">
    <h3>Language Stats</h3>
    <% for (var key in repoLanguages) { %>
      <% if (repoLanguages.hasOwnProperty(key)) { %>
        <p><%=key%> : <%=repoLanguages[key]%></p>
      <%}%>
    <%}%>

    <h3>Social Stats</h3>
    <p><%=repoInfo.stargazers_count%></p>
    <p><%=repoInfo.watchers_count%></p>
  </section>
</div>
